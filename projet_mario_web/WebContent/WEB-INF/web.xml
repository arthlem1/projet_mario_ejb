<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>vingtetun</display-name>
	<context-param>
		<param-name>email</param-name>
		<param-value>vingtetun@ipl.be</param-value>
	</context-param>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<jsp-config>
		<taglib>
			<taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
			<taglib-location>/WEB-INF/tld/c.tld</taglib-location>
		</taglib>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/jspf/header.jspf</include-prelude>
			<include-coda>/WEB-INF/jspf/footer.jspf</include-coda>
		</jsp-property-group>
	</jsp-config>
	<servlet>
		<servlet-name>index.html</servlet-name>
		<jsp-file>/WEB-INF/jsp/index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>index.html</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>attente.html</servlet-name>
		<jsp-file>/WEB-INF/jsp/attente.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>attente.html</servlet-name>
		<url-pattern>/attente.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>refresh.html</servlet-name>
		<jsp-file>/WEB-INF/jspf/refresh.jspf</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>refresh.html</servlet-name>
		<url-pattern>/refresh.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>plateau</servlet-name>
		<jsp-file>/WEB-INF/jsp/plateau.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>plateau.html</servlet-name>
		<jsp-file>/WEB-INF/jspf/plateau.jspf</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>plateau.html</servlet-name>
		<url-pattern>/plateau.html</url-pattern>
	</servlet-mapping>
	<session-config>
		<cookie-config>
			<http-only>true</http-only>
		</cookie-config>
		<tracking-mode>COOKIE</tracking-mode>
	</session-config>
</web-app>